{
    "name": "haikara/flat-route",
    "type": "library",
    "config": {
        "platform": {
          "php": "8.1"
        }
    },
    "autoload": {
        "psr-4": {
            "Haikara\\FlatRoute\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Haikara\\FlatRoute\\": "src/",
            "Haikara\\FlatRoute\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.0 <8.4.0",
        "psr/http-message": "^2.0",
        "psr/container": "^2.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "haikara/middleware-stack": "^0.1.0",
        "haikara/verifier": "^0.1.0"
    },
    "require-dev": {
        "laminas/laminas-diactoros": "^3.3",
        "league/container": "*"
    },
    "repositories": {
        "haikara/middleware-stack": {
          "type": "vcs",
          "url": "https://github.com/HaikaraSakura/middleware-stack"
        },
        "haikara/verifier": {
            "type": "vcs",
            "url": "https://github.com/HaikaraSakura/verifier"
        }
    }
}
